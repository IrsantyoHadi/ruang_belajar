<!DOCTYPE html>
<html lang="en">
    <%- include("../partials/header.ejs")%>
    <%- include("../partials/navbar.ejs")%>
    <script>
        function clickDelete(id) {
          let r = confirm("Kamu yakin mau berenti sekolah???")
          if(r){
              window.location = `/students/delete/${id}` 
          }
        }
    </script>
<body>
  <div class="container">
    <h1>WELCOME <%=dataStudent.first_name%> !!</h1>
    <h2>STUDENT PROFILE</h2>
    <h3><%=msg%></h3>
    <div class="span2">
      <img src="data:image/jpeg;base64,">
    </div>
    <div class="row">
    <div class="span3">
      <blockquote>
        <h3><%=dataStudent.first_name%> <%=dataStudent.last_name%></h3>
      </blockquote>
      <h4>
        Email : <%=dataStudent.email%> <br>
        Age   : <%=dataStudent.age%>
        <br><br>
        <form action="/students/reportCard/<%=dataStudent.id%>" method="get">
          <input type="submit" class="btn btn-warning" value="Get Report">
        </form><br>
        <form action="/students/edit_profile/<%=dataStudent.id%>" method="get">
          <input type="submit" class="btn btn-primary" value="Edit Profile">
        </form>
        <br>
        <button type="submit" class="btn btn-danger" onclick="clickDelete(<%=dataStudent.id%>)">Keluar Sekolah</button>
      </h4>
    </div>
  </div>
</div>
</body>
</html>